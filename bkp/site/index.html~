        <!--form>
            <input id="text" type="text">
            <input onclick="send();" value="Send" type="button">
            <br><br><textarea id="chat" rows="10" cols="50"></textarea>
            <br><br><video autoplay controls poster="" id="video" preload="metadata">Your browser does not support the video element</video>
            <script type="text/javascript">
                var ws = new WebSocket("ws://localhost:8080/ws2udpproj/server");
                var chat = document.getElementById("chat");
                var text = document.getElementById("text");
                var video = document.getElementById("video");
                var stream = new ArrayBuffer(1310);
                var i = 0;
                
                ws.onopen = function() {
                    open();
                };
                
                ws.onmessage = function(msg) {
                    message(msg);
                };
                
                ws.onclose = function() {
                    close();
                };
                
                ws.onerror = function() {
                    error();
                };

                function open() {
                    chat.value += "Connected!\n";
                }
                
                function message(msg) {
                    chat.value += "Receive: " + msg.data + "\n";
                }
                
                function send() {
                    if(text.value !== "close") {
                        ws.send(text.value);
                        chat.value += "Send: " + text.value + "\n";
                        text.value = "";
                    } else ws.close();
                }
                
                function close() {
                    ws.send("Disconnected!");
                    chat.value += "Disconnected!\n";
                }
                
                function error() {
                    chat.value += "Error!\n";
                }

                function play(msg) {
                    if(i === 1310) {
                        var blob = new Blob(stream);
                        video.src = window.URL.createObjectURL(blob);
                        close();
                    }
                    
                    stream[i+1] = msg;
                }
            
            </script>
        </form>
    </body>
